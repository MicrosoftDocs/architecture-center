{
  "baseConfiguration": "docutune-unattended-default.json",
  "onboarding": {
    "maxInFlightPullRequests": 20,
    "maxInFlightFiles": 200
  },
  "pullRequest": {
    "title": "[DocuTune-Remediation] - Scheduled execution to fix known issues in Azure Architecture Center articles",
    "batchSize": 0,
    "maxBatches": 100,
    "body": "",
    "newBranchNamePrefix": "",
    "headRemote": "origin",
    "draft": false,
    "requestReview": true,
    "signOff": false,
    "properties": {
      "pointOfContact": "@ckittel @RobBagby"
    },
    "sleepInterval": 15
  },
  "types": [
    {
      "name": "Default",
      "command": "Fix",
      "configType": "full",
      "stopAt": 25
    },
    {
      "name": "FlagBrokenLinks",
      "base": "Default",
      "configType": "links-only",
      "stopAt": 10
    }
  ]
}

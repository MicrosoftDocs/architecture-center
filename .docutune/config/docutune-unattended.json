{
    "baseConfiguration": "docutune-unattended-default.json",
    "onboarding": {
        "maxInFlightPullRequests": 20,
        "maxInFlightFiles": 200
    },
    "pullRequest": {
        "title": "[BULK] - DocuTune remediation - Content SFI",
        "batchSize": 9,
        "maxBatches": 100,
        "groupByFolder": "docs/*",
        "body": "Remediating articles to align with content SFI guidance related to sensitive terms with GUIDs, thumbprints, and secrets.",
        "newBranchNamePrefix": "",
        "headRemote": "origin",
        "draft": false,
        "requestReview": true,
        "signOff": false,
        "properties": {},
        "sleepInterval": 15
    },
    "types": [
        {
            "name": "Default",
            "command": "Fix",
            "configType": "full"
        }
    ]
}
